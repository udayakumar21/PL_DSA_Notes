2:print a Halloe reactangle
5 5
*****
*   *
*   *
*   *
*****
#include <iostream>

using namespace std;

int main()
{
    int r,c;
    cin>>r;
    cin>>c;
    for(int i=1;i<=r;i++){
        for(int j=1;j<=c;j++){
            if(i==1||i==r||j==1||j==c){
                cout<<"*";
            }
            else{
                cout<<" ";
            }
        }
        cout<<endl;
    }
    return 0;
}
